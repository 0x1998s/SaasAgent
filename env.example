# 环境配置示例文件
# 复制此文件为 .env 并填入真实配置

# 应用环境
ENVIRONMENT=development
DEBUG=true
SECRET_KEY=your-secret-key-here

# 数据库配置
DATABASE_URL=postgresql://saasagent:saasagent123@localhost:5432/saasagent
REDIS_URL=redis://localhost:6379/0
MONGODB_URL=mongodb://saasagent:saasagent123@localhost:27017/saasagent
CHROMADB_URL=http://localhost:8001

# AI模型API配置
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_API_BASE=https://api.openai.com/v1
ANTHROPIC_API_KEY=your-anthropic-api-key-here
OLLAMA_BASE_URL=http://localhost:11434

# 第三方服务API
# 物流跟踪API
AFTERSHIP_API_KEY=your-aftership-api-key
SHIPSTATION_API_KEY=your-shipstation-api-key
FEDEX_API_KEY=your-fedex-api-key
UPS_API_KEY=your-ups-api-key

# 电商平台API
SHOPIFY_API_KEY=your-shopify-api-key
SHOPIFY_SECRET_KEY=your-shopify-secret-key
EBAY_API_KEY=your-ebay-api-key
AMAZON_API_KEY=your-amazon-api-key

# 邮件服务配置
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SENDGRID_API_KEY=your-sendgrid-api-key

# 支付网关
STRIPE_API_KEY=your-stripe-api-key
PAYPAL_CLIENT_ID=your-paypal-client-id
PAYPAL_CLIENT_SECRET=your-paypal-client-secret

# 云服务配置
# Google Cloud Platform
GCP_PROJECT_ID=your-gcp-project-id
GCP_CREDENTIALS_PATH=/path/to/gcp-credentials.json

# AWS配置
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-west-2
AWS_S3_BUCKET=your-s3-bucket

# 监控和日志
SENTRY_DSN=your-sentry-dsn
LOG_LEVEL=INFO
PROMETHEUS_ENABLED=true

# 安全配置
JWT_SECRET_KEY=your-jwt-secret-key
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24
CORS_ORIGINS=http://localhost:3000,https://yourdomain.com

# 缓存配置
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Agent配置
MAX_CONCURRENT_AGENTS=10
AGENT_TIMEOUT_SECONDS=300
MEMORY_RETENTION_DAYS=30

# 速率限制
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST=20
